# 0 "arch/arm64/boot/dts/overlays/wifimac-overlay.dts"
# 0 "<built-in>"
# 0 "<command-line>"
# 1 "arch/arm64/boot/dts/overlays/wifimac-overlay.dts"
/dts-v1/;
/plugin/;

/{
 compatible = "brcm,bcm2835";

 fragment@0 {
  target = <&brcmf>;
  wifi0: __overlay__ {
   local-mac-address = [ 00 00 00 00 00 00 ];
  };
 };

 __overrides__ {
  wifiaddr = <&wifi0>, "local-mac-address[";
 };

 __exports__ {
  wifi0;
 };
};
