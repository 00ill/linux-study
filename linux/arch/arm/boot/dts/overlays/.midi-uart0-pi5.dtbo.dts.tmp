# 0 "arch/arm64/boot/dts/overlays/midi-uart0-pi5-overlay.dts"
# 0 "<built-in>"
# 0 "<command-line>"
# 1 "arch/arm64/boot/dts/overlays/midi-uart0-pi5-overlay.dts"
/dts-v1/;
/plugin/;

# 1 "./scripts/dtc/include-prefixes/dt-bindings/clock/rp1.h" 1
# 5 "arch/arm64/boot/dts/overlays/midi-uart0-pi5-overlay.dts" 2
# 14 "arch/arm64/boot/dts/overlays/midi-uart0-pi5-overlay.dts"
/{
 compatible = "brcm,bcm2712";

 fragment@0 {
  target-path = "/clocks";
  __overlay__ {
   midi_clk: midiclock_0 {
    compatible = "fixed-factor-clock";
    #clock-cells = <0>;
    clocks = <&rp1_clocks 15>;
    clock-mult = <38400>;
    clock-div = <31250>;
   };
  };
 };

 fragment@1 {
  target = <&uart0>;
  __overlay__ {
   status = "okay";
   clocks = <&midi_clk &rp1_clocks 6>;
  };
 };
};
