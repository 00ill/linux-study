# 0 "arch/arm64/boot/dts/overlays/pciex1-compat-pi5-overlay.dts"
# 0 "<built-in>"
# 0 "<command-line>"
# 1 "arch/arm64/boot/dts/overlays/pciex1-compat-pi5-overlay.dts"




/dts-v1/;
/plugin/;

/ {
 compatible = "brcm,bcm2712";


 fragment@0 {
  target = <&pciex1>;
  __dormant__ {
   brcm,clkreq-mode = "default";
  };
 };


 fragment@1 {
  target = <&pciex1>;
  __dormant__ {
   aspm-no-l0s;
  };
 };


 fragment@2 {
  target = <&pciex1>;
  __dormant__ {
   msi-parent = <&pciex1>;
  };
 };






 fragment@3 {
  target = <&pciex1>;
  __dormant__ {
   #address-cells = <3>;
   #size-cells = <2>;
   ranges = <0x02000000 0x00 0x80000000
      0x1b 0x80000000
      0x00 0x7ffffffc>,
     <0x43000000 0x04 0x00000000
      0x18 0x00000000
      0x03 0x80000000>;
  };
 };

 __overrides__ {
  l1ss = <0>, "+0";
  no-l0s = <0>, "+1";
  no-mip = <0>, "+2";
  mmio-hi = <0>, "+3";
 };
};
